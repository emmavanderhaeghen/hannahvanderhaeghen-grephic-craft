{
    "require": {
        "codewithkyle/read-only": "1.0.5",
        "craftcms/cms": "^4.4.0",
        "craftcms/commerce": "4.2.6",
        "craftcms/commerce-mollie": "4.1.0.1",
        "craftcms/guest-entries": "3.0.1",
        "craftcms/postmark": "3.0.0",
        "craftcms/redactor": "3.0.4",
        "misterbk/mix": "1.6.0",
        "nystudio107/craft-seomatic": "4.0.23",
        "putyourlightson/craft-snaptcha": "4.0.1",
        "putyourlightson/craft-sprig": "2.5.1",
        "sebastianlenz/linkfield": "2.1.5",
        "spicyweb/craft-quick-field": "2.0.3",
        "verbb/wishlist": "2.0.4",
        "vlucas/phpdotenv": "^5.4.0"
    },

    "require-dev": {
        "craftcms/generator": "^1.3.0",
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
        "craftcms/plugin-installer": true,
        "yiisoft/yii2-composer": true,
        "php-http/discovery": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
        "php": "8.0.2"
        }
    },
    "scripts": {
        "post-root-package-install": [
        "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
        ]
    }
}
